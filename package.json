{
  "name": "MetaPriv",
  "version": "1.0.10-nightly",
  "description": "An automated tool that allows Facebook users to obfuscate their data and conceal their real interests and habits from Facebook.",
  "main": "main.js",
  "build": {
    "productName": "MetaPriv",
    "appId": "fi.tuni.asif.ibtehaz",
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "bot/**/*",
      "database/**/*",
      "utility/**/*",
      "frontend/build/**/*",
      "assets/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "electron .",
    "debug": "electron --trace-warnings .",
    "bump": "node version-updater.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "react-build": "cd frontend && npm run build",
    "pack": "npm run bump && npm run react-build && electron-builder --dir",
    "dist": "npm run bump && electron-builder install-app-deps && npm run react-build && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherd-06/MetaPriv.git"
  },
  "keywords": [],
  "author": "Ibtehaz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shepherd-06/MetaPriv/issues"
  },
  "homepage": "https://github.com/shepherd-06/MetaPriv#readme",
  "devDependencies": {
    "electron": "^35.4.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.3",
    "create-react-app": "^5.1.0",
    "dotenv": "^16.4.7",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "electron-is-dev": "^3.0.1",
    "find-free-port": "^2.0.0",
    "keytar": "^7.9.0",
    "moment": "^2.30.1",
    "puppeteer": "^24.9.0",
    "random-words": "^2.0.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.3"
  }
}
